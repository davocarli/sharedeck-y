{
	"name": "sharedeck-y",
	"version": "1.1.0",
	"description": "Plugin that loads ShareDeck & SteamDeckHQ reports for your currently-open game.",
	"scripts": {
		"build": "shx rm -rf dist && rollup -c",
		"watch": "rollup -c -w",
		"test": "echo \"Error: no test specified\" && exit 1"
