	"repository": {
		"type": "git",
		"url": "git+https://github.com/SteamDeckHomebrew/decky-plugin-template.git"
	},
	"keywords": [
		"decky",
		"plugin",
		"sharedeck",
		"steamdeckhq",
		"performance",
		"battery",
		"settings",
		"steam-deck",
		"deck"
	],
	"author": "DCA <davocarli@gmail.com>",
	"license": "BSD-3-Clause",
	"bugs": {
		"url": "https://github.com/SteamDeckHomebrew/decky-plugin-template/issues"
	},
	"homepage": "https://github.com/SteamDeckHomebrew/decky-plugin-template#readme",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^21.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-replace": "^4.0.0",
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/react": "16.14.0",
		"@types/webpack": "^5.28.0",
		"rollup": "^2.77.1",
		"rollup-plugin-import-assets": "^1.1.1",
		"shx": "^0.3.4",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
